<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://www.cdisc.org/ns/odm/v2.0" xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:odm="http://www.cdisc.org/ns/odm/v2.0" targetNamespace="http://www.cdisc.org/ns/odm/v2.0"
    xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:xlink="http://www.w3.org/1999/xlink"
    elementFormDefault="qualified" attributeFormDefault="unqualified">

    <xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
    <xs:import namespace="http://www.w3.org/1999/xlink" schemaLocation="xlink.xsd"/>
    <xs:import namespace="http://www.w3.org/1999/xhtml" schemaLocation="ODM-xhtml.xsd"/>
    
    <!--
        +===========================================================================+
        | attribute definition groups                                               |
        | these definitions are NOT to be extended and are internal to ODM USE ONLY |
        +===========================================================================+
    -->
    <xs:attributeGroup name="TranslatedTextAttributeDefinition">
        <xs:attribute ref="xml:lang"/>
        <!-- 
            Added type to support expansion of TranslatedText.
            Type is a Media Type as described in https://www.iana.org/assignments/media-types/media-types.xhtml
            A TranslatedText element with Type="text/plain" must be present. 
        -->
        <xs:attribute name="type" type="text" use="required" />
    </xs:attributeGroup>
    <xs:attributeGroup name="AliasAttributeDefinition">
        <xs:attribute name="Context" type="text" use="required"/>
        <xs:attribute name="Name" type="text" use="required"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="StudyAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="StudyName" type="name" use="required"/>
        <xs:attribute name="ProtocolName" type="name" use="required"/>
        <xs:attribute name="VersionID" type="name" use="optional"/>
        <xs:attribute name="VersionName" type="name" use="optional"/>
        <xs:attribute name="Status" type="name" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="MetaDataVersionAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="StandardAttributeDefinition">
        <xs:annotation><xs:documentation>Definition of a standard referenced in the Define-XML document.</xs:documentation></xs:annotation>
        <xs:attribute name="OID" type="oid" use="required">
            <xs:annotation><xs:documentation>Unique identifier of the version within the XML document.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="Name" type="StandardName"  use="required">
            <xs:annotation><xs:documentation>Name of Standard.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="Type" type="StandardType"  use="required">
            <xs:annotation><xs:documentation>Type of Standard.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="PublishingSet" type="StandardPublishingSet"  use="optional">
            <xs:annotation><xs:documentation>Set of published files of Standard when Type="CT" (e.g. SDTM, ADaM, SEND, CDASH, COA).</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="Version" type="text"  use="required">
            <xs:annotation><xs:documentation>Version of Standard.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="Status" type="StandardStatus"  use="required">
            <xs:annotation><xs:documentation>Status of Standard.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="CommentOID" type="oidref" use="optional">
            <xs:annotation><xs:documentation>The Comment identifier that this value refers to.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>

    <xs:attributeGroup name="ValueListDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required" />    
    </xs:attributeGroup>    
    <xs:attributeGroup name="WhereClauseRefAttributeDefinition">
        <xs:attribute name="WhereClauseOID" type="oidref" use="required" />    
    </xs:attributeGroup>    
    <xs:attributeGroup name="WhereClauseDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required">
            <xs:annotation><xs:documentation>Unique identifier for the WhereClause element.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="CommentOID" type="oidref" use="optional">
            <xs:annotation><xs:documentation>The Comment identifier that this value refers to. Needed when the WhereClause references Items across different domains.
                The Comment would define any join assumptions.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>    
    
    <xs:attributeGroup name="DocumentRefAttributeDefinition">
        <xs:attribute name="leafID" type="xs:IDREF" use="required">
            <xs:annotation><xs:documentation>Unique identifier for the leaf element with the document location.</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="PDFPageRefAttributeDefinition">
        <xs:attribute name="PageRefs" type="text" use="optional">
            <xs:annotation><xs:documentation>List of PDF pages separated by a space.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="FirstPage" type="positiveInteger" use="optional">
            <xs:annotation><xs:documentation>First page in a range of pages.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="LastPage" type="positiveInteger" use="optional">
            <xs:annotation><xs:documentation>Last page in a range of pages.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="Type" type="PDFPageType"  use="required">
            <xs:annotation><xs:documentation>Type of page for page references indicated in the PageRefs attribute.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="Title" type="text" use="optional">
            <xs:annotation><xs:documentation>Text with the label for the document reference.</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>
    <xs:attributeGroup name="leafAttributeDefinition">
        <xs:attribute name="ID" type="xs:ID" use="required">
            <xs:annotation><xs:documentation>Unique identifier for the leaf that is referenced.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute ref="xlink:href" use="required">
            <xs:annotation><xs:documentation>URL that can be used to identify the location of a document or dataset file relative to the folder containing the ODM file.</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>    
    
    <xs:attributeGroup name="StudyEventGroupRefAttributeDefinition">
        <xs:attribute name="StudyEventGroupOID" type="oidref" use="required"/>
        <xs:attribute name="OrderNumber" type="positiveInteger" use="optional"/>
        <xs:attribute name="Mandatory" type="YesOrNo" use="required"/>
        <xs:attribute name="CollectionExceptionConditionOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="StudyEventGroupDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="ArmOID" type="oidref" use="optional"/>
        <xs:attribute name="EpochOID" type="oidref" use="optional"/>
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="StudyEventRefAttributeDefinition">
        <xs:attribute name="StudyEventOID" type="oidref" use="required"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="StudyEventDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="Repeating" type="YesOrNo" use="required"/>
        <xs:attribute name="Type" type="EventType" use="required"/>
        <xs:attribute name="Category" type="text"/>
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ItemGroupRefAttributeDefinition">
        <xs:attribute name="ItemGroupOID" type="oidref" use="required"/>
        <xs:attribute name="MethodOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ItemGroupDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="Repeating" type="ItemGroupRepeatingType" use="required"/>
        <xs:attribute name="RepeatingLimit" type="positiveInteger" use="optional"/>
        <xs:attribute name="IsReferenceData" type="YesOrNo" use="optional"/>
        <xs:attribute name="Structure" type="text" use="optional"/>
        <xs:attribute name="ArchiveLocationID" type="oidref" use="optional"/>
        <xs:attribute name="DatasetName" type="name" use="optional"/>
        <xs:attribute name="Domain" type="text" use="optional"/>
        <xs:attribute name="Type" type="ItemGroupTypeType" use="required"/>
        <xs:attribute name="Purpose" type="text" use="optional"/>
        <xs:attribute name="StandardOID" type="oidref" use="optional"/>        
        <xs:attribute name="IsNonStandard" type="YesOnly" use="optional"/>        
        <xs:attribute name="HasNoData" type="YesOnly" use="optional"/>        
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ClassAttributeDefinition">
        <xs:attribute name="Name" type="ItemGroupClass" use="required" >
            <xs:annotation><xs:documentation>General observation Class.</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>    
    <xs:attributeGroup name="SubClassAttributeDefinition">
        <xs:attribute name="Name" type="ItemGroupSubClass"  use="required">
            <xs:annotation><xs:documentation>General observation Sub Class.</xs:documentation></xs:annotation>
        </xs:attribute>
        <xs:attribute name="ParentClass" type="ItemGroupClassSubClass"  use="optional">
            <xs:annotation><xs:documentation>Parent class of the Sub Class</xs:documentation></xs:annotation>
        </xs:attribute>
    </xs:attributeGroup>    
    
    <xs:attributeGroup name="ItemRefAttributeDefinition">
        <xs:attribute name="ItemOID" type="oidref" use="required"/>
        <xs:attribute name="KeySequence" type="positiveInteger" use="optional"/>
        <xs:attribute name="IsNonStandard" type="YesOnly" use="optional"/>        
        <xs:attribute name="HasNoData" type="YesOnly" use="optional"/>        
        <xs:attribute name="MethodOID" type="oidref" use="optional"/>
        <xs:attribute name="UnitsItemOID" type="oidref" use="optional"/>
        <!-- Repeat attribute signifies that the Item drives the IG repeats based on a code list -->
        <xs:attribute name="Repeat" type="YesOnly" use="optional"/>
        <!-- Other attribute signfies that the Item represents "Other" content added to an ItemGroup -->
        <xs:attribute name="Other" type="YesOnly" use="optional"/>
        <xs:attribute name="Role" type="text" use="optional"/>
        <xs:attribute name="RoleCodeListOID" type="oidref" use="optional"/>
        <xs:attribute name="Core" type="CoreType" use="optional"/>
        <xs:attribute name="PreSpecifiedValue" type="text" use="optional"/>
    </xs:attributeGroup>
      <xs:attributeGroup name="OriginAttributeDefinition">
          <xs:attribute name="Type" type="OriginType" use="required" />        
          <xs:attribute name="Source" type="OriginSource" use="optional" />
      </xs:attributeGroup>    
    <xs:attributeGroup name="SourceItemsAttributeDefinition">
    </xs:attributeGroup>    
    <xs:attributeGroup name="SourceItemAttributeDefinition">
        <xs:attribute name="ItemOID" type="oidref" use="optional"/>
        <xs:attribute name="ItemGroupOID" type="oidref" use="optional"/>
        <xs:attribute name="MetaDataVersionOID" type="oidref" use="optional"/>
        <xs:attribute name="StudyOID" type="oidref" use="optional"/>
        <xs:attribute name="leafID" type="oidref" use="optional"/>
        <xs:attribute name="Name" type="name" use="optional"/>
    </xs:attributeGroup>    
    <xs:attributeGroup name="ResourceAttributeDefinition">
        <xs:attribute name="Type" type="text" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="Attribute" type="text" use="optional"/>
        <xs:attribute name="Label" type="text" use="optional"/>
    </xs:attributeGroup>    
    <xs:attributeGroup name="SelectionAttributeDefinition">
        <xs:attribute name="Path" type="text" use="required"/>
    </xs:attributeGroup>    
    <xs:attributeGroup name="ItemDefAttributeDefinition">
        <xs:annotation>
            <xs:documentation>An ItemDef describes a type of item that can occur within a
                study.</xs:documentation>
        </xs:annotation>
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="DataType" type="DataType" use="required">
            <xs:annotation>
                <xs:documentation>The DataType attribute specifies how the corresponding value
                    elements are to be interpreted for comparison and storage.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Length" type="positiveInteger" use="optional">
            <xs:annotation>
                <xs:documentation>The Length attribute is optional when DataType is text, string,
                    integer, or float; and should not be used for the other
                    datatypes.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="DisplayFormat" type="text" use="optional"/>
        <xs:attribute name="VariableSet" type="text" use="optional"/>
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="RangeCheckAttributeDefinition">
        <xs:attribute name="Comparator" type="Comparator" use="optional"/>
        <xs:attribute name="SoftHard" type="SoftOrHard" use="optional"/>
        <xs:attribute name="ItemOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="CodeListRefAttributeDefinition">
        <xs:attribute name="CodeListOID" type="oidref" use="required"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ValueListRefAttributeDefinition">
        <xs:attribute name="ValueListOID" type="oidref" use="required"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="CodeListAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="DataType" type="CLDataType" use="required"/>
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
        <xs:attribute name="StandardOID" type="oidref" use="optional"/>
        <xs:attribute name="IsNonStandard" type="YesOnly" use="optional"/>        
    </xs:attributeGroup>
    <xs:attributeGroup name="CodeListItemAttributeDefinition">
        <xs:attribute name="CodedValue" type="value" use="required"/>
        <xs:attribute name="Rank" type="decimal" use="optional"/>
        <!-- Other attribute signfies that the term represents "other" content -->
        <xs:attribute name="Other" type="YesOnly" use="optional"/>
        <xs:attribute name="OrderNumber" type="integer" use="optional"/>
        <xs:attribute name="ExtendedValue" type="YesOnly" use="optional"/>        
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ImputationMethodAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="IncludeAttributeDefinition">
        <xs:attribute name="StudyOID" type="oidref" use="required"/>
        <xs:attribute name="MetaDataVersionOID" type="oidref" use="required"/>
        <xs:attribute name="href" type="xs:anyURI" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="RefAttributeSharedDefinition">
        <xs:attribute name="OrderNumber" type="positiveInteger" use="optional"/>
        <xs:attribute name="Mandatory" type="YesOrNo" use="required"/>
        <xs:attribute name="CollectionExceptionConditionOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ExternalSharedAttributeDefinition">
        <xs:attribute name="Dictionary" type="DictionaryNameType" use="optional"/>
        <xs:attribute name="Version" type="text" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="MethodDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="Type" type="MethodType" use="optional"/>
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ParameterAttributeDefinition">
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="DataType" type="DataType" use="required"/>
        <xs:attribute name="Definition" type="text" use="optional"/>
        <xs:attribute name="OrderNumber" type="positiveInteger" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ReturnValueAttributeDefinition">
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="DataType" type="DataType" use="required"/>
        <xs:attribute name="Definition" type="text" use="optional"/>
        <xs:attribute name="OrderNumber" type="positiveInteger" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ConditionDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required"/>
        <xs:attribute name="Name" type="name" use="required"/>
        <xs:attribute name="CommentOID" type="oidref" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="FormalExpressionAttributeDefinition">
        <xs:attribute name="Context" type="text"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="ExternalCodeLibAttributeDefinition">
        <xs:attribute name="Library" type="name" use="required"/>
        <xs:attribute name="Method" type="name" use="optional"/>
        <xs:attribute name="Version" type="text" use="optional"/>
        <xs:attribute name="ref" type="text" use="optional"/>
        <xs:attribute name="href" type="xs:anyURI" use="optional"/>
    </xs:attributeGroup>
    <xs:attributeGroup name="CommentDefAttributeDefinition">
        <xs:attribute name="OID" type="oid" use="required">
            <xs:annotation><xs:documentation>Unique identifier of the comment within the XML document.</xs:documentation></xs:annotation>            
        </xs:attribute>
    </xs:attributeGroup>
    
    <!--
        +===========================================================================+
        | group definitions                                                         |
        +===========================================================================+
    -->
    <xs:group name="StudyEventDefGroup">
        <xs:sequence>
            <xs:element ref="StudyEventGroupRef" minOccurs="0"/>
            <xs:element ref="StudyEventRef" minOccurs="0"/>
        </xs:sequence>
    </xs:group>
    <xs:group name="ItemGroupDefGroup">
        <xs:sequence>
            <xs:element ref="ItemGroupRef" minOccurs="0"/>
            <xs:element ref="ItemRef" minOccurs="0"/>
        </xs:sequence>
    </xs:group>

    <!--
        +===========================================================================+
        | complexType definitions                                                   |
        +===========================================================================+
    -->
    <xs:complexType name="ODMcomplexTypeDefinition-Alias">
        <xs:sequence>
            <xs:group ref="AliasElementExtension" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attributeGroup ref="AliasAttributeDefinition"/>
        <xs:attributeGroup ref="AliasAttributeExtension"/>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-Description">
        <xs:sequence>
            <xs:element ref="TranslatedText" maxOccurs="unbounded"/>
            <xs:group ref="DescriptionElementExtension" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attributeGroup ref="DescriptionAttributeExtension"/>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-TranslatedText" mixed="true">
      <xs:sequence>
        <xs:element ref="xhtml:div" minOccurs="0" maxOccurs="1" />
      </xs:sequence>
      <xs:attributeGroup ref="TranslatedTextAttributeDefinition"/>
      <xs:attributeGroup ref="TranslatedTextAttributeExtension"/>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-Study">
        <xs:sequence>
            <xs:element ref="Description" minOccurs="0"/>
            <xs:element ref="MetaDataVersion" minOccurs="1" maxOccurs="unbounded"/>
            <xs:group ref="StudyElementExtension" maxOccurs="unbounded" minOccurs="0"/>
        </xs:sequence>
        <xs:attributeGroup ref="StudyAttributeDefinition"/>
        <xs:attributeGroup ref="StudyAttributeExtension"/>
    </xs:complexType>

    <xs:complexType name="ODMcomplexTypeDefinition-MetaDataVersion">
        <xs:sequence>
            <xs:group ref="MetaDataVersionPreIncludeElementExtension" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="Description" minOccurs="0"/>
            <xs:element ref="Include" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="Standards" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="AnnotatedCRF" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="SupplementalDoc" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="ValueListDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="WhereClauseDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="Protocol" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="WorkflowDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="StudyEventGroupDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="StudyEventDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="ItemGroupDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="ItemDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="CodeList" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="ConditionDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="MethodDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="CommentDef" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="leaf" minOccurs="0" maxOccurs="unbounded"/>            
            <xs:group ref="MetaDataVersionElementExtension" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attributeGroup ref="MetaDataVersionAttributeDefinition"/>
        <xs:attributeGroup ref="MetaDataVersionAttributeExtension"/>
    </xs:complexType>

    <xs:complexType name="ODMcomplexTypeDefinition-DocumentRef">
        <xs:annotation><xs:documentation>Links to a leaf element with the location of the document.</xs:documentation></xs:annotation>
        <xs:sequence>
            <xs:element ref="PDFPageRef" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation><xs:documentation>The PDFPageRef element is a container for page references in a PDF file.</xs:documentation></xs:annotation>
            </xs:element>
            <xs:group ref="DocumentRefElementExtension" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attributeGroup ref="DocumentRefAttributeDefinition"/>
        <xs:attributeGroup ref="DocumentRefAttributeExtension"/>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-PDFPageRef">
        <xs:annotation><xs:documentation>This element is the container for CRF page references.</xs:documentation></xs:annotation>
        <xs:complexContent>
            <xs:restriction base="xs:anyType">
                <xs:attributeGroup ref="PDFPageRefAttributeDefinition"/>
                <xs:attributeGroup ref="PDFPageRefAttributeExtension"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-leaf">
        <xs:annotation><xs:documentation>Contains the XLink information referenced by DocumentRef or ArchiveLocationID</xs:documentation></xs:annotation>
        <xs:sequence>
            <xs:element name="title" minOccurs="1" maxOccurs="1">
                <xs:annotation><xs:documentation>Text with the label for the document or dataset.</xs:documentation></xs:annotation>
            </xs:element>
            <xs:group ref="leafElementExtension" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attributeGroup ref="leafAttributeDefinition"/>
        <xs:attributeGroup ref="leafAttributeExtension"/>
    </xs:complexType>
    
    <xs:complexType name="ODMcomplexTypeDefinition-Include">
        <xs:sequence>
            <xs:group ref="IncludeElementExtension" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attributeGroup ref="IncludeAttributeDefinition"/>
        <xs:attributeGroup ref="IncludeAttributeExtension"/>
    </xs:complexType>
    
    <xs:complexType name="ODMcomplexTypeDefinition-Standards">
        <xs:sequence>
            <xs:element ref="Standard" minOccurs="1" maxOccurs="unbounded" >
                <xs:annotation><xs:documentation>Definition of a standard referenced in the Define-XML document.</xs:documentation></xs:annotation>                
            </xs:element>            
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-Standard">
        <xs:sequence>
            <xs:group ref="StandardElementExtension" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attributeGroup ref="StandardAttributeDefinition"/>
        <xs:attributeGroup ref="StandardAttributeExtension"/>
    </xs:complexType>
 
    <xs:complexType name="ODMcomplexTypeDefinition-AnnotatedCRF">
        <xs:annotation><xs:documentation>An Annotated Case Report Form (CRF) is a Portable File Format (PDF) document that provides the mapping of data collection fields to the variables or discrete variable values contained within the datasets.</xs:documentation></xs:annotation>
        <xs:sequence>
            <xs:element ref="DocumentRef" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation><xs:documentation>Links to a def:leaf element with the location of the document.</xs:documentation></xs:annotation>
            </xs:element>
            <xs:group ref="AnnotatedCRFElementExtension" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attributeGroup ref="AnnotatedCRFAttributeExtension"/>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-SupplementalDoc">
        <xs:annotation><xs:documentation>Supplemental data definitions</xs:documentation></xs:annotation>
        <xs:sequence>
            <xs:element ref="DocumentRef" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation><xs:documentation>Links to a leaf element with the location of the document.</xs:documentation></xs:annotation>
            </xs:element>
            <xs:group ref="SupplementalDocElementExtension" minOccurs="0" maxOccurs="1"/>
        </xs:sequence>
        <xs:attributeGroup ref="SupplementalDocAttributeExtension"/>
    </xs:complexType>
    <xs:complexType name="ODMcomplexTypeDefinition-ValueListDef">
        <xs:sequence>
            <xs:element ref="Description" minOccurs="0" maxOccurs="1">
                <xs:annotation><xs:documentation>Description of the value list.</xs:documentation></xs:annotation>
            </xs:element>
            <xs:element ref="ItemRef" minOccurs="1" maxOccurs="unbounded">
